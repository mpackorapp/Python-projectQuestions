def Reverse(lst):
    def islist(l): 
        return isinstance(l, list)
    new_list = []
    for e in lst:
        if isinstance(e, list):
            new_list.append(Reverse(e))
        else:
            new_list.append(e)
    new_list.reverse()
    return new_list
mylist = [[1, 2], [3, 4], [5, 6, 7]]
print(Reverse(mylist))
